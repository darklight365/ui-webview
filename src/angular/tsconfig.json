{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "rootDir": "./",
        "paths": {
            "tns-core-modules": ["./node_modules/@nativescript/core"],
            "tns-core-modules/*": ["./node_modules/@nativescript/core/*"],
            "@nativescript-community/ui-webview": ["plugin/webview-ext"],
            "@nativescript-community/ui-webview/*": ["packages/*"]
        }
    },
    "include": ["./**/*.ts", "../../../references.d.ts", "../../references.d.ts"],
    "angularCompilerOptions": {
        "enableIvy": true
    }
}
